{
  "name": "table-printer-cli",
  "version": "0.0.0",
  "description": "console table printer cli. Print table on bash terminal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ctp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "lint": "eslint --ext=js,ts .",
    "format": "prettier --write \"**/*.{json,md,ts,tsx,yml,js,jsx}\"",
    "test": "jest --config jestconfig.json",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ayonious/table-printer-cli.git"
  },
  "keywords": [
    "console-table",
    "console-log",
    "print-table",
    "cli"
  ],
  "author": "Nahiyan Kamal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayonious/table-printer-cli/issues"
  },
  "homepage": "https://console-table.netlify.app/docs/doc-cli-install-quick-start",
  "dependencies": {
    "commander": "^10.0.0",
    "console-table-printer": "^2.11.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^20.1.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
