{
  "name": "table-printer-cli",
  "version": "0.0.0",
  "description": "console table printer cli. Print table on bash terminal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ctp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "lint": "eslint --ext=js,ts .",
    "format": "prettier --write \"**/*.{json,md,ts,tsx,yml,js,jsx}\"",
    "test": "jest --config jestconfig.json",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ayonious/table-printer-cli.git"
  },
  "keywords": [
    "console-table",
    "console-log",
    "print-table",
    "cli"
  ],
  "author": "Nahiyan Kamal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayonious/table-printer-cli/issues"
  },
  "homepage": "https://github.com/ayonious/table-printer-cli#readme",
  "dependencies": {
    "commander": "^7.2.0",
    "console-table-printer": "^2.9.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.3",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
