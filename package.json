{
  "name": "table-printer-cli",
  "version": "0.0.0",
  "description": "console table printer cli. Print table on bash terminal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ctp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "lint": "eslint --ext=js,ts .",
    "format": "prettier --write \"**/*.{json,md,ts,tsx,yml,js,jsx}\"",
    "test": "jest --config jestconfig.json",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ayonious/table-printer-cli.git"
  },
  "keywords": [
    "console-table",
    "console-log",
    "print-table",
    "cli"
  ],
  "author": "Nahiyan Kamal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayonious/table-printer-cli/issues"
  },
  "homepage": "https://github.com/ayonious/table-printer-cli#readme",
  "dependencies": {
    "commander": "^6.2.0",
    "console-table-printer": "^2.5.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.31",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
